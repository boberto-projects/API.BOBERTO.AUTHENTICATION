{
  "ConnectionStrings": {
    "PostgreeConnectionContext": "%DATABASE_URL%",
    "RedisConnectionContext": "%REDIS_URL%"
  },
  "ApiConfig": {
    "Swagger": false,
    "Authorization": {
      "Activate": true,
      "ApiHeader": "%API_HEADER%",
      "ApiKey": "%API_KEY%"
    }
  },
  "ResourcesConfig": {
    "Resources": [
      {
        "Key": "PreferirDiscordAoSMS",
        "Enabled": true
      }
    ]
  },
  "SmtpConfig": {
    "Host": "%SMTP_HOST%",
    "From": "%SMTP_FROM%",
    "Port": "%SMTP_PORT%",
    "Username": "%SMTP_USERNAME%",
    "Password": "%SMTP_PASSWORD%"
  },
  "Jwt": {
    "Key": "minhachavesecreta",
    "Issuer": "AlgumIssuer",
    "Audience": "AlgumaAudience"
  },
  "TwoFactorConfig": {
    "Key": "%OTP_KEY%",
    "Size": 6,
    "Step": 60
  },
  "GerenciadorAutenticacaoConfig": {
    "SegundosExpiracao": 86400,
    "QuantidadeMaximaTentativas": 3
  },
  "GerenciadorZenvioConfig": {
    "SegundosExpiracao": 86400,
    "QuantidadeMaximaTentativas": 3
  },
  "DiscordApiConfig": {
    "Enabled": true,
    "Url": "https://discord.com/api",
    "WebHookId": "%DISCORD_WEBHOOK_ID%",
    "WebHookToken": "%DISCORD_WEBHOOK_TOKEN%"
  },
  "ZenviaApiConfig": {
    "Url": "https://api.zenvia.com/v2",
    "ApiKey": "%ZENVIA_APIKEY%",
    "Alias": "%ZENVIA_ALIAS%",
    "Enabled": true
  },
  "AllowedHosts": "*"
}